{
  "name": "sn-script-debugger-enhanced",
  "version": "1.0.0",
  "description": "Advanced debugging and development tools for ServiceNow developers",
  "main": "src/background/index.js",
  "scripts": {
    "dev": "webpack --mode development --watch",
    "build": "webpack --mode production",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.{js,jsx}",
    "lint:fix": "eslint src/**/*.{js,jsx} --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,css,json}\"",
    "zip": "node scripts/zip.js",
    "version:bump": "node scripts/version-bump.js"
  },
  "keywords": [
    "servicenow",
    "debugger",
    "developer-tools",
    "chrome-extension",
    "gliderecord",
    "performance-monitoring"
  ],
  "author": "Tomoki Gozu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gozu52/sn-script-debugger-enhanced.git"
  },
  "bugs": {
    "url": "https://github.com/gozu52/sn-script-debugger-enhanced/issues"
  },
  "homepage": "https://github.com/gozu52/sn-script-debugger-enhanced#readme",
  "dependencies": {
    "idb": "^7.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.10.0",
    "react-syntax-highlighter": "^15.5.0",
    "date-fns": "^2.30.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.0",
    "babel-loader": "^9.1.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "style-loader": "^3.3.3",
    "html-webpack-plugin": "^5.5.3",
    "eslint": "^8.52.0",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^3.0.3",
    "jest": "^29.7.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^6.1.4"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": ["<rootDir>/tests/setup.js"]
  }
}