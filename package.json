{
  "name": "sn-script-debugger-enhanced",
  "version": "1.0.0",
  "description": "Advanced debugging and development tools for ServiceNow developers",
  "scripts": {
    "dev": "webpack --mode development --watch",
    "build": "webpack --mode production",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.{js,jsx}",
    "lint:fix": "eslint src/**/*.{js,jsx} --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,css,json}\"",
    "zip": "node scripts/zip.js",
    "version:bump": "node scripts/version-bump.js"
  },
  "keywords": [
    "servicenow",
    "debugger",
    "developer-tools",
    "chrome-extension",
    "gliderecord",
    "performance-monitoring"
  ],
  "author": "Tomoki Gozu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gozu52/sn-script-debugger-enhanced.git"
  },
  "bugs": {
    "url": "https://github.com/gozu52/sn-script-debugger-enhanced/issues"
  },
  "homepage": "https://github.com/gozu52/sn-script-debugger-enhanced#readme",
  "dependencies": {
    "idb": "^8.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^2.12.7",
    "react-syntax-highlighter": "^15.5.0",
    "date-fns": "^3.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "babel-loader": "^9.1.3",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.1",
    "style-loader": "^4.0.0",
    "html-webpack-plugin": "^5.6.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "prettier": "^3.2.5",
    "jest": "^29.7.0",
    "@testing-library/react": "^15.0.0",
    "@testing-library/jest-dom": "^6.4.2"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": ["<rootDir>/tests/setup.js"]
  }
}